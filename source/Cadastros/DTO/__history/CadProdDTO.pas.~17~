unit CadProdDTO;
{$pointermath on}
{$scopedenums on}

interface
   uses
      System.Generics.Collections,
      System.Types,
      System.SysUtils;
type
  TDtoCadProduto = class;

  TDtoCadProdutos = class;

  TDtoCadProduto = class (TObjectList<TDtoCadProdutos>)
    procedure InternalClear;
  private
    FDescricao2: System.UnicodeString;
    FDescricao: System.UnicodeString;
    FCodReduzido: System.UnicodeString;
    FAtvia: System.UnicodeString;
    FID: System.Integer;
    FEmpresa: System.UnicodeString;
    FUsaCentCusto: System.UnicodeString;
    FUsuario: System.UnicodeString;
    FCodigoCC: System.UnicodeString;
    FTipo: System.UnicodeString;
    FGrupo: System.UnicodeString;

  protected

  public
    procedure Clear; inline;
//    constructor Create(const AOwner: EcoRT.Objects.TErtPersistent);override;
  published
    property ID            : System.Integer       read FID            write FID;
    property Empresa       : System.UnicodeString read FEmpresa       write FEmpresa;
    property CodigoCC      : System.UnicodeString read FCodigoCC      write FCodigoCC;
    property Grupo         : System.UnicodeString read FGrupo         write FGrupo;
    property Descricao     : System.UnicodeString read FDescricao     write FDescricao;
    property CodReduzido   : System.UnicodeString read FCodReduzido   write FCodReduzido;
    property Usuario       : System.UnicodeString read FUsuario       write FUsuario;
    property Tipo          : System.UnicodeString read FTipo          write FTipo;
    property Descricao2    : System.UnicodeString read FDescricao2    write FDescricao2;
    property UsaCentCusto  : System.UnicodeString read FUsaCentCusto  write FUsaCentCusto;
    property Ativa         : System.UnicodeString read FAtvia         write FAtiva;

  end;

  TDtoCadProdutos = class
    (TObjectList<TDtoCadProduto>)
  end;
implementation

